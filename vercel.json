{
  "github": {
    "silent": true
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/git",
      "status": 301,
      "headers": {
        "Location": "https://github.com/useverto"
      }
    },
    {
      "src": "/chat",
      "status": 301,
      "headers": {
        "Location": "https://discord.gg/RnWbc8Y"
      }
    },
    {
      "src": "/login",
      "status": 301,
      "headers": {
        "Location": "/"
      }
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ]
}